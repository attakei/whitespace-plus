{
    "name": "whitespace-plus",
    "displayName": "Whitespace+",
    "description": "Clearly mark whitespace characters.",
    "version": "0.0.2",
    "publisher": "davidhouchin",
    "readme": "README.md",
    "license": "LICENSE",
    "keywords": [
        "whitespace"
    ],
    "bugs": {
        "url": "https://www.github.com/davidhouchin/whitespace-plus/issues"
    },
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.enableWhitespacePlus"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [{
            "command": "extension.enableWhitespacePlus",
            "title": "Enable Whitespace-Plus"
        },{
            "command": "extension.disableWhitespacePlus",
            "title": "Disable Whitespace-Plus"
        }]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "vscode": "^0.11.0"
    }
}