{
    "name": "whitespace-plus",
    "displayName": "whitespace-plus",
    "description": "Clearly mark whitespace characters.",
    "version": "1.0.0",
    "publisher": "davidghouchin",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.enableWhitespacePlus"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [{
            "command": "extension.enableWhitespacePlus",
            "title": "Enable Whitespace-Plus"
        },{
            "command": "extension.disableWhitespacePlus",
            "title": "Disable Whitespace-Plus"
        }]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "vscode": "^0.11.0"
    }
}